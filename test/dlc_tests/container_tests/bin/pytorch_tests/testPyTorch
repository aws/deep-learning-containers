#!/bin/bash

HOME_DIR=/test
BIN_DIR=${HOME_DIR}/bin
LOG_DIR=${HOME_DIR}/logs

git clone https://github.com/pytorch/examples.git ${HOME_DIR}/artifacts/examples
timeout 5m ${BIN_DIR}/pytorch_tests/testPyTorchHelper # Proceed a preparation and exit in advance in case training gets stuck
${BIN_DIR}/pytorch_tests/testPyTorchHelper || exit 1
exit 0
