#!/bin/bash

set -e

HOME_DIR=/test
BIN_DIR=${HOME_DIR}/bin
LOG_DIR=${HOME_DIR}/logs

source ${BIN_DIR}/pytorch_tests/setupPyTorchBackendTest

AWS_LOG_DIR=$LOG_DIR/aws_gloo_log/
OSS_LOG_DIR=$LOG_DIR/oss_gloo_log/

USE_INDUCTOR=1 WORLD_SIZE=2 PT_BACKEND=gloo OUT_PATH="./aws_res.csv" timeout 360 python run.py BERT_pytorch -m eager -d $DEVICE -t train

exit 0